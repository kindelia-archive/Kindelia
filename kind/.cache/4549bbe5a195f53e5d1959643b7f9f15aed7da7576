#world #file ((file #file Bits) #file.name #file.ownr #file.type #file.expr !name = (Kindelia.serialize.name file.name); !ownr = (((Kindelia.serialize.list String) Kindelia.serialize.name) file.ownr); !type = ((Kindelia.serialize.type world) file.type); !expr = (((Kindelia.serialize.term world) (List.nil String)) file.expr); ((Bits.concat name) ((Bits.concat ownr) ((Bits.concat type) expr))))