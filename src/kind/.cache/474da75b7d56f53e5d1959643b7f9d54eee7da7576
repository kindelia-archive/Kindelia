#world #bond ((bond #bond Bits) #bond.name #bond.input_names #bond.input_types #bond.output_type #bond.main #bond.owners !name = ((Litereum.serialize.name world) bond.name); !input_names = (((Litereum.serialize.list String) (Litereum.serialize.name world)) bond.input_names); !input_types = (((Litereum.serialize.list Litereum.Type) (Litereum.serialize.type world)) bond.input_types); !output_type = ((Litereum.serialize.type world) bond.output_type); !main = (((Litereum.serialize.term world) ((List.reverse String) bond.input_names)) bond.main); !owners = (((Litereum.serialize.list String) (Litereum.serialize.name world)) bond.owners); ((Bits.concat name) ((Bits.concat input_names) ((Bits.concat input_types) ((Bits.concat output_type) ((Bits.concat main) owners))))))